{
  "extends": "./tsconfig-base.json",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    // "declaration": true,
    // "declarationDir": "./types",
    // "declarationMap": true,
    "lib": [
      "ESNext"
    ],
    "outDir": "./dist",
    "paths": {
      "#@": ["./package.json"],
      "#*": ["./src/*.ts"],
    },
    "rootDir": ".",
    "tsBuildInfoFile": ".cache/tsconfig.tsbuildinfo",
    "typeRoots": [
      "./node_modules/@types",
    ],
    "types": [
      "node",
    ],
  },
}
